{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"emitDeclarationOnly": false,
		"outDir": "dist",
		"rootDir": "src"
	},
	"include": ["src"]
}
